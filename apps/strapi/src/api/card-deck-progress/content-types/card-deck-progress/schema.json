{
  "kind": "collectionType",
  "collectionName": "card_deck_progresses",
  "info": {
    "singularName": "card-deck-progress",
    "pluralName": "card-deck-progresses",
    "displayName": "Card Deck Progress",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "studentState": {
      "type": "enumeration",
      "enum": [
        "idle",
        "in-progress",
        "completed"
      ]
    },
    "teacherState": {
      "type": "enumeration",
      "enum": [
        "in-review",
        "assesment-confirmed"
      ]
    },
    "currentCard": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::card.card"
    },
    "school": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::school.school"
    },
    "cardDeck": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::card-deck.card-deck",
      "inversedBy": "cardDeckProgresses"
    },
    "student": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::student.student",
      "inversedBy": "cardDeckProgresses"
    }
  }
}
